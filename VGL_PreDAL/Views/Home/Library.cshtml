@{
}
@model IEnumerable<VideoGame>

<html>
    <div class="text-center">
        <h1 class="display-4">@ViewData["Title"]</h1>
    </div>

    <body>
    @if (Model == null)
    {
            <h2>No games provided</h2>
    }
    else
    {
        foreach (VideoGame game in Model)
        {
                    <partial name="../Video Game/_Library.cshtml" for="@game" />
        }
    }
    </body>
</html>